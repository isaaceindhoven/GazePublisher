{
    "name": "isaac/gazepublisher",
    "type": "library",
    "license": "proprietary",
    "description": "Publish library for GazeHub",
    "authors": [
        {
            "name": "ISAAC",
            "email": "info@isaac.nl",
            "homepage": "https://www.isaac.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "ISAAC\\GazePublisher\\": "src/"
        }
    },
    "require": {
        "php": "~7.3.0 || ~7.4.0 || ~8.0.0",
        "ext-curl": "*",
        "ext-json": "*",
        "firebase/php-jwt": "^5.2",
        "isaac/composer-git-hooks-standard": "^1.3",
        "phpunit/phpunit": "^9.5"
    },
    "require-dev": {
        "isaac/composer-git-hooks": "^1.2",
        "isaac/php-code-sniffer-standard": "^21.0",
        "nunomaduro/collision": "^5.4",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.84",
        "phpstan/phpstan-strict-rules": "^0.12.9"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "repositories": {
        "isaac": {
            "type": "composer",
            "url": "https://composer-packages.hq.isaac.nl/repository"
        }
    }
}
